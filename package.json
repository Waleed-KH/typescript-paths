{
  "name": "typescript-paths",
  "version": "1.2.3",
  "description": "Resolving tsconfig paths in runtime",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "author": "lightyen",
  "repository": {
    "url": "https://github.com/lightyen/typescript-paths"
  },
  "homepage": "https://github.com/lightyen/typescript-paths#readme",
  "scripts": {
    "types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly  --outDir types",
    "test": "jest",
    "build": "rollup -c",
    "format": "prettier --config .prettierrc.yml --write {src,tests,types}/**/*",
    "prepare": "jest && rollup -c && yarn types && yarn format",
    "doit": "yarn upgrade-interactive --latest"
  },
  "keywords": [
    "typescript",
    "tsconfig",
    "paths"
  ],
  "peerDependencies": {
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.6",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "rollup": "2.58.3",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "files": [
    "dist",
    "types",
    "register.js",
    "LICENSE",
    "README.md"
  ]
}
