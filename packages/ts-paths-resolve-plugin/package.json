{
  "name": "ts-paths-resolve-plugin",
  "version": "5.7.0",
  "description": "A webpack resolve plugin for tsconfig-paths",
  "license": "MIT",
  "types": "types/index.d.ts",
  "main": "./lib/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "node": "./lib/index.js"
    },
    "./esm": "./lib/esm/index.js"
  },
  "author": "lightyen",
  "repository": {
    "url": "https://github.com/lightyen/typescript-paths"
  },
  "homepage": "https://github.com/lightyen/typescript-paths/blob/main/packages/ts-paths-resolve-plugin#readme",
  "scripts": {
    "types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir types",
    "compile": "rollup -c",
    "format": "prettier --config ../../.prettierrc.yml --write index.ts types/**/*",
    "build": "npm run compile && npm run types && npm run format",
    "prepare": "npm run build"
  },
  "dependencies": {
    "typescript-paths": "^1.4.0"
  },
  "peerDependencies": {
    "webpack": "^5"
  },
  "devDependencies": {
    "enhanced-resolve": "^5.9.0",
    "webpack": "^5"
  },
  "keywords": [
    "webpack",
    "plugin",
    "tsconfig",
    "paths"
  ],
  "files": [
    "lib",
    "types",
    "LICENSE",
    "README.md"
  ]
}
